{
    "presets": [
        "react",
        [
            "es2015",
            {
                "modules": false
            }
        ],
        "stage-1"
    ],
    "plugins": [
        "transform-async-to-generator",
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-runtime",
        "transform-decorators-legacy",
        "lodash",
        ["import", { libraryName: "antd", style: "css" }]
    ]
}